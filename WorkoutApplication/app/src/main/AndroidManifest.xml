        </activity>
        <activity android:name=".ExercisesList.ExercisesListActivity" />
        <activity android:name=".Home.HomeActivity" />
        <activity android:name=".Registration.RegistrationActivity" />

        <uses-permission android:name="android.permission.INTERNET" />

        <activity android:name=".Tabs.TabsActivity" />
        <activity android:name=".WorkoutCreate.WorkoutCreateActivity"></activity>
    </application>

</manifest>